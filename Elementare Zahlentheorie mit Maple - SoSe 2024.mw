<Group hide-input="false" labelreference="L29" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Primzusammengesetz:=proc(n)
  local i,j,k,Q;
<Font encoding="UTF-8">  i:=1; #Anfangspunkt des Z\303\244hlers
</Font>  j:=1; #Anfangspunkt des Nenners
  for k from 2 to n do
    if isprime(k) then i:=i*k
     else j:=j*k
   fi
  od;
 Q:=i/j
end proc:</Text-field>
</Input>
