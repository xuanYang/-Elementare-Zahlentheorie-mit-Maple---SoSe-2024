<Group hide-input="false" labelreference="L35" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GbFor5bis1000:=proc()
local n,gb,i,b,y:
 gb := true:
 for n from 6  to 1000 by 2  do
 i:=3: b:=false:
while i&lt;=n do
 y:=n-i;
 if isprime(y) and y&lt;&gt;2 then
   i:=n+1: b:=true
 end if;
 i:=nextprime(i)
 end do;
 if b=false then
   gb:=false: print(n)
  end if
 end do;
 print(gb)
end proc;

 
GbFor5bis1000();</Text-field>
