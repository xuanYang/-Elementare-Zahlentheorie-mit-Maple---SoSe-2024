<Group hide-input="false" labelreference="L6" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Summe:=proc(n)
  local i,s;
  s:=0; i:=0;
  while i&lt;n do 
        i:=i+1;
        s:= s+i
 end do;
 print(s)
end proc;</Text-field>
